    "@tailwindcss/forms": "0.5.7",
    "firebase": "10.7.0",
    "next": "14.0.3",
    "next-sitemap": "4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-email": "1.9.5",
    "react-hook-form": "7.48.2",
